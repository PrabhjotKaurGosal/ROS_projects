<?xml version="1.0" encoding="UTF-8" ?>
<launch>

<!-- launch Gazebo world - Cafe_actors.world-->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="verbose" value="true"/>
    <arg name="world_name" value="$(find socially_compliant_bot)/worlds/Cafe_actors.world"/> 
  </include>

<!-- launch Gazebo world - ISCAS_museum_actors.world -->
<!--
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="verbose" value="true"/>
    <arg name="world_name" value="$(find socially_compliant_bot)/worlds/ISCAS_museum_actors.world"/> 
  </include>
-->


<!-- spawn a robot in the world -->
    <arg name="x" default="-2.0" />
    <arg name="y" default="0.0" />
    <arg name="z" default="0.0" />
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find socially_compliant_bot)/urdf/summit_xl.urdf -urdf -x $(arg x) -y $(arg y) -z $(arg z) -model summit_xl" /> 

</launch>
